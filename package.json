{
	"name": "pkg-name",
	"version": "0.0.0",
	"packageManager": "pnpm@8.8.0",
	"author": "Ray <i@mk1.io> (@so1ve)",
	"description": "",
	"keywords": [],
	"homepage": "https://github.com/so1ve/pkg-name#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/so1ve/pkg-name.git"
	},
	"bugs": {
		"url": "https://github.com/so1ve/pkg-name/issues"
	},
	"license": "MIT",
	"sideEffects": false,
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "unbuild",
		"lint": "eslint . && prettier . --check",
		"lint:fix": "eslint . --fix && prettier . --write",
		"prepublishOnly": "nr build",
		"release": "bumpp --commit --push --tag && pnpm publish",
		"start": "tsx src/index.ts",
		"stub": "unbuild --stub",
		"test": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@eslint-community/eslint-utils": "^4.4.0",
		"@typescript-eslint/type-utils": "^6.7.4",
		"@typescript-eslint/types": "^6.7.4",
		"@typescript-eslint/utils": "^6.7.4",
		"ts-api-utils": "^1.0.3"
	},
	"devDependencies": {
		"@antfu/ni": "^0.21.8",
		"@so1ve/eslint-config": "^1.0.0-alpha.8",
		"@so1ve/prettier-config": "^1.0.0-alpha.8",
		"@types/lodash.merge": "^4.6.7",
		"@types/node": "^20.8.3",
		"@typescript-eslint/typescript-estree": "6.7.4",
		"ajv": "^6.10.0",
		"bumpp": "^9.2.0",
		"dedent": "^1.5.1",
		"eslint": "^8.51.0",
		"lodash.merge": "4.6.2",
		"pkgroll": "^2.0.1",
		"prettier": "^3.0.3",
		"semver": "^7.5.4",
		"tsx": "^3.13.0",
		"typescript": "^5.2.2",
		"unbuild": "^2.0.0",
		"vite": "^4.4.11",
		"vitest": "^0.34.6"
	},
	"peerDependencies": {
		"typescript": "^5.2.2"
	}
}
